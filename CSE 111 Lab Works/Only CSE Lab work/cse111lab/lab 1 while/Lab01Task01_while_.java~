import java.util.Scanner;
public class Lab01Task01_while
{
  public static void main(String [] args)
  {
    Scanner sc=new Scanner(System.in);
    System.out.println("PLease enter a number");
    int num;
    num=sc.nextInt();
    int c;
    
    whilec=1;
    {
      c<=num;
      {
        System.out.print("*");
      }
      c++
    }
  }
}
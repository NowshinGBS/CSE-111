import java.util.Scanner;
public class lab3task02
{
  public static void main(String[] args)
  {
    Scanner sc= new Scanner(System.in);
    int [] a= new int[10];
    int c;
    for(c=0;c<=9;c++)
    {
     
      a[c]=sc.nextInt();
            System.out.print("you have entered ");
            for(int i=0; i<=c; i++)
            {  
              System.out.print(a[i]+" ");
            
   if(i== c)
            {
            System.out.print(a[i] + ".");
            }
   else
   {
     System.out.println(",");
   }
            }
    }
  }
}


    
   
    
  

